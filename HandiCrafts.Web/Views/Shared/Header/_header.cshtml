<style>
    .sidenav {
        height: 100%;
        width: 0;
        position: fixed;
        z-index: 1;
        top: 0;
        left: 0;
        background-color: #111;
        overflow-x: hidden;
        transition: 0.5s;
        padding-top: 60px;
    }

        .sidenav a {
            padding: 8px 8px 8px 32px;
            text-decoration: none;
            font-size: 25px;
            color: #818181;
            display: block;
            transition: 0.3s;
        }

            .sidenav a:hover {
                color: #f1f1f1;
            }

        .sidenav .closebtn {
            position: absolute;
            top: 0;
            right: 25px;
            font-size: 36px;
            margin-left: 50px;
        }

    @@media screen and (max-height: 450px) {
        .sidenav {
            padding-top: 15px;
        }

            .sidenav a {
                font-size: 18px;
            }
    }

    .curtain, .curtainrl {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        opacity: 0.3;
        background-color: #000;
        -webkit-transition: left .4s ease;
        -moz-transition: left .4s ease;
        -o-transition: left .4s ease;
        transition: left .4s ease;
        visibility: hidden;
        z-index: 1033;
    }

    @@media only screen and (max-width: 991px) {
        #mySidenav.speed-in {
            left: 0;
            background-color: white;
            width: 320px;
            position: fixed;
            height: 100%;
            z-index: 100001;
            -webkit-transition: left .4s ease;
            -moz-transition: left .4s ease;
            -o-transition: left .4s ease;
            transition: left .4s ease;
        }

        #searchresult {
            background: #ebecee !important;
        }
    }

    @@media only screen and (max-width: 991px) {
        #mySidenav {
            -webkit-transition: left .4s ease;
            -moz-transition: left .4s ease;
            -o-transition: left .4s ease;
            transition: left .4s ease;
            top: 0;
            background-color: white;
            left: -320px;
            background: #fff;
            position: fixed;
            width: 320px;
            height: 100%;
            z-index: 100001;
        }

        button.cart-close-btn {
            border: 0;
            background: 0;
            padding: 0px 20px;
            position: absolute;
            top: 0;
            right: 0;
            outline: 0;
            color: white;
        }

        #searchresult {
            background: #ebecee !important;
        }

        .searchbtnAllsite{
            display:none!important;
        }
    }

    .curtainrl {
        position: fixed;
        top: 0;
        left: 0;
        height: 100%;
        opacity: 0;
        background-color: #000;
        -webkit-transition: left .4s ease;
        -moz-transition: left .4s ease;
        -o-transition: left .4s ease;
        transition: left .4s ease;
        visibility: hidden;
        z-index: 1033;
    }

    .curtainrl, .robin-page-ckeckout {
        width: 100%;
    }

    .sidenav h4 {
        background-color: #4dc4ca;
        color: white;
        padding: 10px;
        text-align: center;
        margin: 0;
        position: relative;
        box-shadow: 0 0 3px 2px #dfebec;
    }

    .sidecheckout {
        text-align: center !important;
        color: #fff !important;
        background: #b98f2b !important;
    }

        .sidecheckout:hover {
            color: #fff !important;
            background: #5bb623 !important;
        }

    .cd-cart-items {
        position: fixed;
        bottom: 0px;
        width: 270px;
        background: #4dc4ca;
        color: white;
        font-weight: bold;
    }

    .saray-shopping-basket-mob {
